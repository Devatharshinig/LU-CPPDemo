<< LEAP YEAR>>
#include <iostream>

using namespace std;
int main()
{
    int yr;
    cout<<"Enter year "<<endl;
    cin>>yr;
    if(yr%4==0)
    cout<<yr<< "  is a leap year";
    else
    cout<<yr<< "  is a leap year";
    return 0;
}
__________________________________________________

<<FIBONACCI>>

#include <iostream>

using namespace std;

int main()
{
    
    int n,t1=0,t2=1,t3;
    cout<<"Enter n ";
    cin>>n;
    cout<<t1<< " "<<t2;
    for(int i=2;i<n;i++){
        t3=t1+t2;
        cout<<" "<<t3;
        t1=t2;
        t2=t3;
    }
    return 0;
}
____________________________________________________________________________________
<<FACTORIAL>

#include <iostream>

using namespace std;
int main()
{
    int n,i,f=1;
    cout<<"Enter n ";
    cin>>n;
    for(int i=1;i<=n;i++){
        f=f*i;
    }
    cout<<"\n The factorial of "<<n<<" is "<<f;
    return 0;
}
________________________________________________________________
<<mULTIPLICATION TABLE>>

#include <iostream>
using namespace std;
int main()
{
    int n,i;
    cout<<"Enter n ";
    cin>>n;
    cout<<"\n The Multiplication table of "<<n<<" is below";
    for(int i=1;i<=10;i++){
        cout<<"\n"<<i<<" * "<<n<<" = "<<(n*i);
    }

    return 0;
}
